    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/debug/test-harness.out",
            "args": ["-s", "../baseline/heaan/server/debug/heaan-server-main.out", "-c", 
                "../baseline/heaan/client/debug/heaan-client-main.out", "-t", "debug/heaan/TEST_8/test.txt", "-d", "log"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}